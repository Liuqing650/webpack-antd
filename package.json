  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --progress --config webpack.config.js --open",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3300
      }
    },
    "build": {
      "command": "webpack --progress --config webpack.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3600
      }
    },
    "analyzer": {
      "command": "webpack --progress --json > stats.json --config webpack.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "ANALYZER": true,
        "PORT": 3600
      }
    }
  },
